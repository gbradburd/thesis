keywords = {Africa,Animals,DNA,DNA: genetics,Dentin,Dentin: chemistry,Elephants,Elephants: genetics,Geography,Microsatellite Repeats},
month = oct,
number = {41},
pages = {14847--52},
pmid = {15459317},
title = {Assigning {African} elephant {DNA} to geographic region of origin: {Applications} to the ivory trade.},
volume = {101},
year = {2004}
}

