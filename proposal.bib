	file = {PubMed Central Full Text PDF:/Users/gcoop79/Library/Application Support/Firefox/Profiles/0t235o0e.default/zotero/storage/25HRRCKA/Patterson et al. - 2012 - Ancient Admixture in Human History.pdf:application/pdf}
